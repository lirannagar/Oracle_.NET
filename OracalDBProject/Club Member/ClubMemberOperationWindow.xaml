<Window x:Class="OracalDBProject.Club_Member.ClubMemberOperationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OracalDBProject.Club_Member"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Club Member Operation" Height="300" Width="300">
    <Grid Margin="0,0,3.6,-0.4">
        <Button x:Name="StartBuying" Content="Start buying" HorizontalAlignment="Left" Margin="65,48,0,0" VerticalAlignment="Top" Width="141" Height="30" Click="StartBuying_Click"/>
        <Button x:Name="Exit" Content="Exit" HorizontalAlignment="Left" Margin="65,91,0,0" VerticalAlignment="Top" Width="141" Height="30" Click="Exit_Click"/>
    </Grid>
</Window>
