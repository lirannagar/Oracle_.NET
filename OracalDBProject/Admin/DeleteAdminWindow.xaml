<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OracalDBProject.Admin"
        xmlns:OracalDBProject="clr-namespace:OracalDBProject" x:Class="OracalDBProject.Admin.DeleteAdminWindow"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="DeleteAdminWindow" Height="300" Width="300" Loaded="Window_Loaded">
    <Window.Resources>
        <OracalDBProject:MarketsDBSource x:Key="marketsDBSource"/>
        <CollectionViewSource x:Key="uSERSViewSource" Source="{Binding USERS, Source={StaticResource marketsDBSource}}"/>
    </Window.Resources>
    <Grid DataContext="{StaticResource uSERSViewSource}">
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Margin="10,101,0,0" VerticalAlignment="Top" Height="159" Width="274" ItemsSource="{Binding}"/>

    </Grid>
</Window>
